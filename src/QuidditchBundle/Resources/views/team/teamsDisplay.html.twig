{% for team in teams %}
	<tr>
		<td><a href="{{ path('team_show', { 'id': team.id }) }}">{{ team.id }}</a></td>
		<td>{{ team.name }}</td>
		<td>{{ team.country }}</td>
		<td>{{ team.totalExp }}</td>
		<td>{{ team.averageExhaust | round(2) }}</td>
		<td>
			<ul>
				<li>
					<a href="{{ path('team_show', { 'id': team.id }) }}">show</a>
				</li>
				<li>
					<a href="{{ path('team_edit', { 'id': team.id }) }}">edit</a>
				</li>
			</ul>
		</td>
	</tr>
{% endfor %}