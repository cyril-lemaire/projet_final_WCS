<!DOCTYPE html>
    <html>
    <head>
        <meta charset="UTF-8" />
        <title>{% block title %}Welcome!{% endblock %}</title>
            {% block stylesheets %}{% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/css/materialize.min.css">
        <link rel="stylesheet" href="{{ asset('bundles/quidditch/style/style.css') }}">
    </head>
    <body>
        {% block body %}{% endblock %}
        {% block javascripts %}
            <script src="{{ asset('bundles/quidditch/javascript/jquery.min.js') }}" />
            {# That'd be cool to load the cdn by default and the local version if it couldn't load#}
            {#<script#}
                {#src="http://code.jquery.com/jquery-3.1.1.min.js"#}
                {#integrity="sha256-hVVnYaiADRTO2PzUGmuLJr8BLUSjGIZsDYGmIJLv2b8="#}
                {#crossorigin="anonymous"></script>#}
            <!-- Compiled and minified JavaScript -->
            <script src="https://cdnjs.cloudflare.com/ajax/libs/materialize/0.97.6/js/materialize.min.js"></script>
            <script>
                $(document).ready(function(){
                	$('select').show(); // Override materialize's automatic select.hide()
                });
            </script>
        {% endblock %}
    </body>
</html>